{
  "name": "api-node",
  "version": "1.0.0",
  "description": "Exemplo de API REST usando node e typescript",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "launch": "node dist/index.js",
    "start": "npm run build && npm run launch",
    "dev": "tsc -w & nodemon",
    "preview": "ts-node src/index.ts",
    "format": "prettier --write \"**/*.{js,cjs,mjs,json,ts}\"",
    "lint": "eslint \"**/*.{js,cjs,mjs,ts}\"",
    "lint:fix": "eslint \"**/*.{js,cjs,mjs,ts}\" --fix",
    "fix": "npm run format && npm run lint:fix",
    "clear": "rm -rf dist"
  },
  "author": "",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql2": "^3.2.0"
  }
}
